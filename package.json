{
  "name": "nuxt",
  "version": "0.1.0",
  "description": "Opinionated architecture and dev environment for new Nuxt.js projects.",
  "repository": "https://github.com/davidroyer/nuxt-boilerplate.git",
  "author": "David Royer <droyer01@gmail.com>",
  "private": false,
  "browserslist": [
    ">1%",
    "not ie 11",
    "not op_mini all"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development nuxt",
    "build": "cross-env NODE_ENV=production nuxt build",
    "start": "cross-env NODE_ENV=production nuxt start",
    "generate": "nuxt generate",
    "lint:js": "cross-env NODE_ENV=production eslint --ext .js,.vue .",
    "lint:css": "cross-env NODE_ENV=production stylelint **/*.{vue,css,scss,sass,less}",
    "lint": "npm-run-all lint:js lint:css"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "^3.0.0-beta.11",
    "@vue/eslint-config-prettier": "^3.0.0-beta.11",
    "@vue/eslint-config-standard": "^3.0.0-beta.11",
    "autoprefixer": "^8.5.1",
    "cross-env": "^5.0.1",
    "css-mqpacker": "^6.0.2",
    "glob-all": "^3.1.0",
    "husky": "^0.14.3",
    "lodash": "^4.17.10",
    "npm-run-all": "^4.1.3",
    "path": "^0.12.7",
    "postcss-animation": "^0.0.12",
    "postcss-easing-gradients": "^3.0.0",
    "postcss-import": "^11.1.0",
    "postcss-nested": "^3.0.0",
    "postcss-preset-env": "^5.1.0",
    "postcss-responsive-type": "^1.0.0",
    "purgecss-webpack-plugin": "^1.3.1",
    "stylelint": "^9.2.1",
    "stylelint-config-prettier": "^3.2.0",
    "stylelint-config-recess-order": "^2.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^0.10.5"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-brands-svg-icons": "^5.3.1",
    "@fortawesome/free-regular-svg-icons": "^5.3.1",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@nuxtjs/pwa": "^2.6.0",
    "@nuxtjs/sitemap": "^0.1.1",
    "nuxt": "^2.1.0",
    "nuxt-fontawesome": "^0.3.0",
    "postcss-typography": "^1.0.0",
    "tailwindcss": "^0.5.3"
  }
}
